{
  "plugins": [[
    "module-resolver",
    {
      "root": ["./src"],
      "extensions": [
        ".ios.ts",
        ".android.ts",
        ".ts",
        ".ios.tsx",
        ".android.tsx",
        ".tsx",
        ".jsx",
        ".js",
        ".json"
      ],
      "alias": {
        "components": "./src/components",
        "config": "./src/config",
        "hooks": "./src/hooks",
        "pkgs": "./src/pkgs",
        "providers": "./src/providers",
      }
    }
  ]
  ]
}
